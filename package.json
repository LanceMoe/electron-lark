{
  "name": "electron-lark-typescript",
  "version": "1.1.6",
  "description": "An Electron application for Feishu(Lark)",
  "main": "src/main.js",
  "linux": {
    "category": "Office",
    "packageCategory": "Office",
    "synopsis": "An Electron application for Feishu(Lark)",
    "icon": "./build/icons",
    "target": [
      {
        "target": "deb",
        "arch": [
          "x64"
        ]
      }
    ],
    "desktop": {
      "Encoding": "UTF-8"
    }
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "start": "pnpm build && cd dist && electron ."
  },
  "repository": "https://github.com/LanceMoe/electron-lark",
  "keywords": [
    "Electron",
    "typescript",
    "Feishu",
    "Lark",
    "飞书"
  ],
  "author": "LanceMoe <admin@lance.moe>",
  "license": "CC0-1.0",
  "devDependencies": {
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "electron": "^20.0.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.7.1",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "electron-context-menu": "^3.5.0"
  }
}
